{
    "manifest_version": 2,
    "name": "UnifyPR",
    "version": "1.0",
  
    "description": "Pull request (PR) manager",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "permissions": [
        "storage",
        "https://api.github.com/",
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
  